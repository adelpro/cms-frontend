{
  "name": "cms-frontend",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:production": "ng build --configuration production",
    "build:staging": "ng build --configuration staging",
    "build:develop": "ng build --configuration develop",
    "test": "ng test",
    "lint": "ng lint"
